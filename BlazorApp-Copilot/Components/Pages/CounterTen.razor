@page "/counter10"
@rendermode InteractiveServer

<PageTitle>Counter by 10</PageTitle>

<h1>Counter by 10</h1>

<p role="status">Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Count by 10</button>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount += 10;
    }
}
